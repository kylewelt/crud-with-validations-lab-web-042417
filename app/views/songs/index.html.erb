<h1>All Songs</h1>

<table>
  <tbody>
  <% @songs.each do |song| %>
    <tr>
      <td>
        <%= link_to song.title, song_path(song) %>
      </td>
      <td>
        <%= song.release_year %>
      </td>
      <td>
        <%= song.released %>
      </td>
      <td>
        <%= song.genre %>
      </td>
      <td>
        <%= song.artist_name %>
      </td>
      <td>
        <%= link_to "Delete", song, method: :delete %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>
